<Window
    x:Class="GTA5OnlineTools.Modules.Windows.BigBaseV2Window"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Name="Window_BigBaseV2"
    Title="BigBaseV2 - STAT编辑/解锁"
    Closing="Window_BigBaseV2_Closing"
    Loaded="Window_BigBaseV2_Loaded"
    Style="{StaticResource WindowNormalStyle}"
    WindowStartupLocation="CenterScreen"
    mc:Ignorable="d">

    <TabControl
        Name="TabControl_Main"
        Grid.Row="0"
        Background="White"
        TabStripPlacement="Left">
        <TabControl.Resources>
            <Style TargetType="TabItem">
                <Setter Property="Padding" Value="10,7,10,7" />
                <Setter Property="Background" Value="#F5F5F6" />
            </Style>
        </TabControl.Resources>

        <TabItem Header="主页">
            <ScrollViewer Style="{StaticResource ScrollViewerVAHDStyle}">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition />
                        <ColumnDefinition />
                    </Grid.ColumnDefinitions>

                    <GroupBox
                        Grid.Column="0"
                        Margin="5"
                        Header="功能简介"
                        Style="{StaticResource GroupBoxWebStyle}">
                        <RichTextBox BorderThickness="0" Style="{StaticResource PageRichTextBoxStyle}">
                            <FlowDocument>
                                <Paragraph Style="{StaticResource TitleParagraphStyle}">
                                    简介
                                </Paragraph>
                                <Paragraph Style="{StaticResource ContentParagraphStyle}">
                                    有的数据是需要计算的，比如：总收入=现金+银行+分类收入（差事+下注+分成+车辆销售额+拾取金额+其他...）
                                </Paragraph>
                                <Paragraph Style="{StaticResource ContentParagraphStyle}">
                                    其它数据也是类似的原理，而不是乱改一气，否则所有可能产生的风险将由玩家自行承担
                                </Paragraph>
                                <Paragraph Style="{StaticResource ContentParagraphStyle}">
                                    MP0_这种类型的数据区分角色，如果读不出来，可以尝试把MP0改为MP1
                                </Paragraph>
                                <Paragraph Style="{StaticResource ContentParagraphStyle}">
                                    Social Club 线上角色数据查询<LineBreak />
                                    <Hyperlink NavigateUri="https://socialclub.rockstargames.com/games/gtav/pc/career/overview/gtaonline" RequestNavigate="Hyperlink_RequestNavigate">
                                        https://socialclub.rockstargames.com/games/gtav/pc/career/overview/gtaonline
                                    </Hyperlink>
                                </Paragraph>

                                <!--<Paragraph Style="{StaticResource TitleParagraphStyle}">
                                        提示
                                    </Paragraph>
                                    <Paragraph Style="{StaticResource ContentParagraphStyle}">
                                        ● 详细使用教程请查看小助手帮助
                                        <LineBreak/>
                                        <Hyperlink NavigateUri="https://crazyzhang.cn" RequestNavigate="Hyperlink_RequestNavigate">
                                            https://crazyzhang.cn
                                        </Hyperlink>
                                    </Paragraph>-->
                            </FlowDocument>
                        </RichTextBox>
                    </GroupBox>

                    <GroupBox
                        Grid.Column="1"
                        Margin="5"
                        Header="DLL注入器"
                        Style="{StaticResource GroupBoxWebStyle}">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>

                            <Grid Margin="5">
                                <Border Style="{StaticResource BorderEffectStyle}" />
                                <DockPanel LastChildFill="True">
                                    <StackPanel
                                        Margin="10,10,0,0"
                                        DockPanel.Dock="Top"
                                        Orientation="Horizontal">
                                        <TextBlock
                                            Margin="0,0,5,0"
                                            Style="{StaticResource IconFontInfoTextBlockStyle}"
                                            Text="&#xe624;" />
                                        <TextBlock
                                            FontSize="18"
                                            FontWeight="Bold"
                                            Text="操作日志" />
                                    </StackPanel>
                                    <TextBox
                                        x:Name="TextBox_Log"
                                        Margin="5"
                                        AcceptsReturn="False"
                                        BorderBrush="Gray"
                                        BorderThickness="0"
                                        Style="{StaticResource TextBoxNormalStyle}"
                                        TextWrapping="WrapWithOverflow" />
                                </DockPanel>
                            </Grid>

                            <StackPanel Grid.Row="1">
                                <GroupBox
                                    Margin="5"
                                    Header="选择你要注入的DLL（鼠标悬浮菜单名称可以预览）"
                                    Style="{StaticResource GroupBoxWebStyle}">
                                    <WrapPanel Orientation="Horizontal">
                                        <RadioButton
                                            x:Name="RadioButton_InjectDLL_Bread"
                                            Margin="5,5,10,5"
                                            Click="RadioButton_InjectDLL_Bread_Click"
                                            Content="Bread"
                                            IsChecked="False">
                                            <RadioButton.ToolTip>
                                                <Image Source="/Assets/Images/Tips/Bread.png" Style="{StaticResource ImageHighQualityStyle}" />
                                            </RadioButton.ToolTip>
                                        </RadioButton>
                                        <RadioButton
                                            x:Name="RadioButton_InjectDLL_Bread_Chs"
                                            Margin="5,5,10,5"
                                            Click="RadioButton_InjectDLL_Bread_Click"
                                            Content="Bread汉化版">
                                            <RadioButton.ToolTip>
                                                <Image Source="/Assets/Images/Tips/Bread_Chs.png" Style="{StaticResource ImageHighQualityStyle}" />
                                            </RadioButton.ToolTip>
                                        </RadioButton>
                                        <RadioButton
                                            x:Name="RadioButton_InjectDLL_PackedStatEditor"
                                            Margin="5,5,10,5"
                                            Click="RadioButton_InjectDLL_Bread_Click"
                                            Content="Packed Stat Editor"
                                            IsChecked="True">
                                            <RadioButton.ToolTip>
                                                <Image Source="/Assets/Images/Tips/PackedStatEditor.png" Style="{StaticResource ImageHighQualityStyle}" />
                                            </RadioButton.ToolTip>
                                        </RadioButton>
                                    </WrapPanel>
                                </GroupBox>

                                <TextBlock
                                    Margin="5"
                                    Foreground="Red"
                                    TextWrapping="Wrap">
                                    警告：为了安全起见，请在故事模式注入，然后再进入线上
                                </TextBlock>
                                <TextBlock Margin="5" TextWrapping="Wrap">
                                    提示：注入成功后，按 INSERT 键显示/隐藏菜单。在对应菜单中点击 Unload 按钮退出菜单（请勿关闭对应控制台窗口）
                                </TextBlock>
                                <Button
                                    x:Name="Button_BigBaseV2_Inject"
                                    Click="Button_BigBaseV2_Inject_Click"
                                    Content="注入DLL到GTA5进程"
                                    IsEnabled="True"
                                    Style="{StaticResource ButtonNormalStyle}" />
                                <Button
                                    x:Name="Button_Located_Stats"
                                    Click="Button_Located_Stats_Click"
                                    Content="打开BigBaseV2配置文件夹"
                                    Style="{StaticResource ButtonNormalStyle}" />
                                <TextBlock
                                    Margin="5"
                                    VerticalAlignment="Center"
                                    FontSize="18"
                                    FontWeight="Bold"
                                    Foreground="Red">
                                    警告：非必要请勿使用此工具，可能会有风险
                                </TextBlock>

                            </StackPanel>

                        </Grid>
                    </GroupBox>
                </Grid>
            </ScrollViewer>
        </TabItem>

        <TabItem Header="生涯">
            <ScrollViewer Style="{StaticResource ScrollViewerVAHDStyle}">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition />
                        <ColumnDefinition />
                        <ColumnDefinition />
                    </Grid.ColumnDefinitions>

                    <GroupBox
                        Grid.Column="0"
                        Margin="5"
                        Header="代码部分1"
                        Style="{StaticResource GroupBoxWebStyle}">
                        <StackPanel Margin="5" Orientation="Vertical">
                            <TextBlock FontWeight="Bold" Text="全球声望值排名 - short" />
                            <TextBox Style="{StaticResource TextBoxInputStyle}" Text="MPPLY_GLOBALXP">
                                <TextBox.ToolTip>
                                    <TextBlock Text="暂无提示" />
                                </TextBox.ToolTip>
                            </TextBox>

                            <TextBlock FontWeight="Bold" Text="总收入 - s64" />
                            <TextBox Style="{StaticResource TextBoxInputStyle}" Text="MPPLY_TOTAL_EVC">
                                <TextBox.ToolTip>
                                    <TextBlock Text="暂无提示" />
                                </TextBox.ToolTip>
                            </TextBox>

                            <TextBlock FontWeight="Bold" Text="总花费 - s64" />
                            <TextBox Style="{StaticResource TextBoxInputStyle}" Text="MPPLY_TOTAL_SVC">
                                <TextBox.ToolTip>
                                    <TextBlock Text="暂无提示" />
                                </TextBox.ToolTip>
                            </TextBox>

                            <TextBlock FontWeight="Bold" Text="杀死的玩家总数 - int" />
                            <TextBox Style="{StaticResource TextBoxInputStyle}" Text="MPPLY_KILLS_PLAYERS">
                                <TextBox.ToolTip>
                                    <TextBlock Text="暂无提示" />
                                </TextBox.ToolTip>
                            </TextBox>

                            <TextBlock FontWeight="Bold" Text="被其他玩家杀死的总次数 - int" />
                            <TextBox Style="{StaticResource TextBoxInputStyle}" Text="MPPLY_DEATHS_PLAYER">
                                <TextBox.ToolTip>
                                    <TextBlock Text="暂无提示" />
                                </TextBox.ToolTip>
                            </TextBox>

                            <TextBlock FontWeight="Bold" Text="玩家对战击杀/死亡比 - float" />
                            <TextBox Style="{StaticResource TextBoxInputStyle}" Text="MPPLY_KILL_DEATH_RATIO">
                                <TextBox.ToolTip>
                                    <TextBlock Text="暂无提示" />
                                </TextBox.ToolTip>
                            </TextBox>

                        </StackPanel>
                    </GroupBox>

                    <GroupBox
                        Grid.Column="1"
                        Margin="5"
                        Header="代码部分2"
                        Style="{StaticResource GroupBoxWebStyle}">
                        <StackPanel Margin="5" Orientation="Vertical">
                            <TextBlock FontWeight="Bold" Text="行走距离 - float" />
                            <TextBox Style="{StaticResource TextBoxInputStyle}" Text="MPPLY_CHAR_DIST_TRAVELLED">
                                <TextBox.ToolTip>
                                    <TextBlock Text="数值单位为米，例如：需修改成1千米则输入1000" />
                                </TextBox.ToolTip>
                            </TextBox>

                            <TextBlock FontWeight="Bold" Text="最喜爱的无线电台 - int" />
                            <TextBox Style="{StaticResource TextBoxInputStyle}" Text="MPPLY_MOST_FAVORITE_STATION">
                                <TextBox.ToolTip>
                                    <TextBlock Text="输入电台对应的哈希值（具体哈希值未知）" />
                                </TextBox.ToolTip>
                            </TextBox>

                            <TextBlock FontWeight="Bold" Text="GTA在线模式中花费的时间 - u64" />
                            <TextBox Style="{StaticResource TextBoxInputStyle}" Text="MP_PLAYING_TIME">
                                <TextBox.ToolTip>
                                    <TextBlock Text="数值单位为毫秒，例如：需修改成1小时则输入3600000" />
                                </TextBox.ToolTip>
                            </TextBox>

                            <TextBlock FontWeight="Bold" Text="以第一人称视角进行游戏的时间 - u64" />
                            <TextBox Style="{StaticResource TextBoxInputStyle}" Text="MP_FIRST_PERSON_CAM_TIME">
                                <TextBox.ToolTip>
                                    <TextBlock Text="数值单位为毫秒，例如：需修改成1小时则输入3600000" />
                                </TextBox.ToolTip>
                            </TextBox>

                            <TextBlock FontWeight="Bold" Text="死斗游戏中花费的时间 - u64" />
                            <TextBox Style="{StaticResource TextBoxInputStyle}" Text="MPPLY_TOTAL_TIME_SPENT_DEATHMAT">
                                <TextBox.ToolTip>
                                    <TextBlock Text="数值单位为毫秒，例如：需修改成1小时则输入3600000" />
                                </TextBox.ToolTip>
                            </TextBox>

                            <TextBlock FontWeight="Bold" Text="竞速中花费的时间 - u64" />
                            <TextBox Style="{StaticResource TextBoxInputStyle}" Text="MPPLY_TOTAL_TIME_SPENT_RACES">
                                <TextBox.ToolTip>
                                    <TextBlock Text="数值单位为毫秒，例如：需修改成1小时则输入3600000" />
                                </TextBox.ToolTip>
                            </TextBox>

                            <TextBlock FontWeight="Bold" Text="制作器中花费的时间 - u64" />
                            <TextBox Style="{StaticResource TextBoxInputStyle}" Text="MPPLY_TOTAL_TIME_MISSION_CREATO">
                                <TextBox.ToolTip>
                                    <TextBlock Text="数值单位为毫秒，例如：需修改成1小时则输入3600000" />
                                </TextBox.ToolTip>
                            </TextBox>

                        </StackPanel>
                    </GroupBox>

                    <GroupBox
                        Grid.Column="2"
                        Margin="5"
                        Header="代码部分3"
                        Style="{StaticResource GroupBoxWebStyle}">
                        <StackPanel Margin="5" Orientation="Vertical">
                            <TextBlock FontWeight="Bold" Text="死斗游戏发布次数 - int" />
                            <TextBox Style="{StaticResource TextBoxInputStyle}" Text="MPPLY_AWD_FM_CR_DM_MADE">
                                <TextBox.ToolTip>
                                    <TextBlock Text="暂无提示" />
                                </TextBox.ToolTip>
                            </TextBox>

                            <TextBlock FontWeight="Bold" Text="竞速发布次数 - int" />
                            <TextBox Style="{StaticResource TextBoxInputStyle}" Text="MPPLY_AWD_FM_CR_RACES_MADE">
                                <TextBox.ToolTip>
                                    <TextBlock Text="暂无提示" />
                                </TextBox.ToolTip>
                            </TextBox>

                            <TextBlock FontWeight="Bold" Text="夺取发布次数 - int" />
                            <TextBox Style="{StaticResource TextBoxInputStyle}" Text="MPPLY_NUM_CAPTURES_CREATED">
                                <TextBox.ToolTip>
                                    <TextBlock Text="暂无提示" />
                                </TextBox.ToolTip>
                            </TextBox>

                            <TextBlock FontWeight="Bold" Text="团队生存游戏发布次数 - int" />
                            <TextBox Style="{StaticResource TextBoxInputStyle}" Text="MPPLY_LTS_CREATED">
                                <TextBox.ToolTip>
                                    <TextBlock Text="暂无提示" />
                                </TextBox.ToolTip>
                            </TextBox>

                            <TextBlock FontWeight="Bold" Text="他人游玩您发布的内容的次数 - int" />
                            <TextBox Style="{StaticResource TextBoxInputStyle}" Text="MPPLY_AWD_FM_CR_PLAYED_BY_PEEP">
                                <TextBox.ToolTip>
                                    <TextBlock Text="暂无提示" />
                                </TextBox.ToolTip>
                            </TextBox>

                            <TextBlock FontWeight="Bold" Text="您发布的内容所获好评数 - int" />
                            <TextBox Style="{StaticResource TextBoxInputStyle}" Text="MPPLY_FM_MISSION_LIKES">
                                <TextBox.ToolTip>
                                    <TextBlock Text="暂无提示" />
                                </TextBox.ToolTip>
                            </TextBox>

                        </StackPanel>
                    </GroupBox>

                </Grid>
            </ScrollViewer>
        </TabItem>

        <TabItem Header="技能">
            <ScrollViewer Style="{StaticResource ScrollViewerVAHDStyle}">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition />
                        <ColumnDefinition />
                        <ColumnDefinition />
                    </Grid.ColumnDefinitions>

                    <GroupBox
                        Grid.Column="0"
                        Margin="5"
                        Header="代码部分1"
                        Style="{StaticResource GroupBoxWebStyle}">
                        <StackPanel Margin="5" Orientation="Vertical">
                            <TextBlock FontWeight="Bold" Text="体力 - int" />
                            <TextBox Style="{StaticResource TextBoxInputStyle}" Text="MP0_SCRIPT_INCREASE_STAM">
                                <TextBox.ToolTip>
                                    <TextBlock Text="数值范围0~100" />
                                </TextBox.ToolTip>
                            </TextBox>

                            <TextBlock FontWeight="Bold" Text="射击技能 - int" />
                            <TextBox Style="{StaticResource TextBoxInputStyle}" Text="MP0_SCRIPT_INCREASE_SHO">
                                <TextBox.ToolTip>
                                    <TextBlock Text="数值范围0~100" />
                                </TextBox.ToolTip>
                            </TextBox>

                            <TextBlock FontWeight="Bold" Text="力量 - int" />
                            <TextBox Style="{StaticResource TextBoxInputStyle}" Text="MP0_SCRIPT_INCREASE_STRN">
                                <TextBox.ToolTip>
                                    <TextBlock Text="数值范围0~100" />
                                </TextBox.ToolTip>
                            </TextBox>

                            <TextBlock FontWeight="Bold" Text="潜行 - int" />
                            <TextBox Style="{StaticResource TextBoxInputStyle}" Text="MP0_SCRIPT_INCREASE_STL">
                                <TextBox.ToolTip>
                                    <TextBlock Text="数值范围0~100" />
                                </TextBox.ToolTip>
                            </TextBox>

                            <TextBlock FontWeight="Bold" Text="飞行技能 - int" />
                            <TextBox Style="{StaticResource TextBoxInputStyle}" Text="MP0_SCRIPT_INCREASE_FLY">
                                <TextBox.ToolTip>
                                    <TextBlock Text="数值范围0~100" />
                                </TextBox.ToolTip>
                            </TextBox>

                            <TextBlock FontWeight="Bold" Text="驾驶技能 - int" />
                            <TextBox Style="{StaticResource TextBoxInputStyle}" Text="MP0_SCRIPT_INCREASE_DRIV">
                                <TextBox.ToolTip>
                                    <TextBlock Text="数值范围0~100" />
                                </TextBox.ToolTip>
                            </TextBox>

                            <TextBlock FontWeight="Bold" Text="肺活量 - int" />
                            <TextBox Style="{StaticResource TextBoxInputStyle}" Text="MP0_SCRIPT_INCREASE_LUNG">
                                <TextBox.ToolTip>
                                    <TextBlock Text="数值范围0~100" />
                                </TextBox.ToolTip>
                            </TextBox>

                            <TextBlock FontWeight="Bold" Text="精神状态 - float" />
                            <TextBox Style="{StaticResource TextBoxInputStyle}" Text="MP0_PLAYER_MENTAL_STATE">
                                <TextBox.ToolTip>
                                    <TextBlock Text="未知" />
                                </TextBox.ToolTip>
                            </TextBox>

                        </StackPanel>
                    </GroupBox>

                    <GroupBox
                        Grid.Column="1"
                        Margin="5"
                        Header="代码部分2"
                        Style="{StaticResource GroupBoxWebStyle}">
                        <StackPanel Margin="5" Orientation="Vertical" />
                    </GroupBox>

                    <GroupBox
                        Grid.Column="2"
                        Margin="5"
                        Header="代码部分3"
                        Style="{StaticResource GroupBoxWebStyle}">
                        <StackPanel Margin="5" Orientation="Vertical" />
                    </GroupBox>

                </Grid>
            </ScrollViewer>
        </TabItem>

        <TabItem Header="通用">
            <ScrollViewer Style="{StaticResource ScrollViewerVAHDStyle}">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition />
                        <ColumnDefinition />
                        <ColumnDefinition />
                    </Grid.ColumnDefinitions>

                    <GroupBox
                        Grid.Column="0"
                        Margin="5"
                        Header="代码部分1"
                        Style="{StaticResource GroupBoxWebStyle}">
                        <StackPanel Margin="5" Orientation="Vertical">
                            <TextBlock FontWeight="Bold" Text="第三人称视角游戏时间 - u64" />
                            <TextBox Style="{StaticResource TextBoxInputStyle}" Text="MP0_TOTAL_PLAYING_TIME">
                                <TextBox.ToolTip>
                                    <TextBlock Text="数值单位为毫秒，例如：需修改成1小时则输入3600000" />
                                </TextBox.ToolTip>
                            </TextBox>

                            <TextBlock FontWeight="Bold" Text="制作的角色 - date" />
                            <TextBox Style="{StaticResource TextBoxInputStyle}" Text="MP0_CHAR_DATE_CREATED">
                                <TextBox.ToolTip>
                                    <TextBlock Text="暂无提示" />
                                </TextBox.ToolTip>
                            </TextBox>

                            <TextBlock FontWeight="Bold" Text="最后一次升级 - date" />
                            <TextBox Style="{StaticResource TextBoxInputStyle}" Text="MP0_CHAR_DATE_RANKUP">
                                <TextBox.ToolTip>
                                    <TextBlock Text="暂无提示" />
                                </TextBox.ToolTip>
                            </TextBox>

                            <TextBlock FontWeight="Bold" Text="持续时间最长单人战局 - u64" />
                            <TextBox Style="{StaticResource TextBoxInputStyle}" Text="MP0_LONGEST_PLAYING_TIME">
                                <TextBox.ToolTip>
                                    <TextBlock Text="数值单位为毫秒，例如：需修改成1小时则输入3600000" />
                                </TextBox.ToolTip>
                            </TextBox>

                            <TextBlock FontWeight="Bold" Text="每场战局平均用时 - u64" />
                            <TextBox Style="{StaticResource TextBoxInputStyle}" Text="MP0_AVERAGE_TIME_PER_SESSON">
                                <TextBox.ToolTip>
                                    <TextBlock Text="数值单位为毫秒，例如：需修改成1小时则输入3600000" />
                                </TextBox.ToolTip>
                            </TextBox>

                            <TextBlock Margin="5" />

                            <TextBlock FontWeight="Bold" Text="死亡总数 - int" />
                            <TextBox Style="{StaticResource TextBoxInputStyle}" Text="MP0_DEATHS">
                                <TextBox.ToolTip>
                                    <TextBlock Text="暂无提示" />
                                </TextBox.ToolTip>
                            </TextBox>

                            <TextBlock FontWeight="Bold" Text="爆炸死亡数 - int" />
                            <TextBox Style="{StaticResource TextBoxInputStyle}" Text="MP0_DIED_IN_EXPLOSION">
                                <TextBox.ToolTip>
                                    <TextBlock Text="暂无提示" />
                                </TextBox.ToolTip>
                            </TextBox>

                            <TextBlock FontWeight="Bold" Text="坠落死亡数 - int" />
                            <TextBox Style="{StaticResource TextBoxInputStyle}" Text="MP0_DIED_IN_FALL">
                                <TextBox.ToolTip>
                                    <TextBlock Text="暂无提示" />
                                </TextBox.ToolTip>
                            </TextBox>

                            <TextBlock FontWeight="Bold" Text="火烧死亡数 - int" />
                            <TextBox Style="{StaticResource TextBoxInputStyle}" Text="MP0_DIED_IN_FIRE">
                                <TextBox.ToolTip>
                                    <TextBlock Text="暂无提示" />
                                </TextBox.ToolTip>
                            </TextBox>

                        </StackPanel>
                    </GroupBox>

                    <GroupBox
                        Grid.Column="1"
                        Margin="5"
                        Header="代码部分2"
                        Style="{StaticResource GroupBoxWebStyle}">
                        <StackPanel Margin="5" Orientation="Vertical">
                            <TextBlock FontWeight="Bold" Text="撞死次数 - int" />
                            <TextBox Style="{StaticResource TextBoxInputStyle}" Text="MP0_DIED_IN_ROAD">
                                <TextBox.ToolTip>
                                    <TextBlock Text="暂无提示" />
                                </TextBox.ToolTip>
                            </TextBox>

                            <TextBlock FontWeight="Bold" Text="溺水死亡数 - int" />
                            <TextBox Style="{StaticResource TextBoxInputStyle}" Text="MP0_DIED_IN_DROWNING">
                                <TextBox.ToolTip>
                                    <TextBlock Text="暂无提示" />
                                </TextBox.ToolTip>
                            </TextBox>

                            <TextBlock Margin="5" />

                            <TextBlock FontWeight="Bold" Text="游泳时间 - u64" />
                            <TextBox Style="{StaticResource TextBoxInputStyle}" Text="MP0_TIME_SWIMMING">
                                <TextBox.ToolTip>
                                    <TextBlock Text="数值单位为毫秒，例如：需修改成1小时则输入3600000" />
                                </TextBox.ToolTip>
                            </TextBox>

                            <TextBlock FontWeight="Bold" Text="游泳距离 - float" />
                            <TextBox Style="{StaticResource TextBoxInputStyle}" Text="MP0_DIST_SWIMMING">
                                <TextBox.ToolTip>
                                    <TextBlock Text="数值单位为米，例如：需修改成1千米则输入1000" />
                                </TextBox.ToolTip>
                            </TextBox>

                            <TextBlock FontWeight="Bold" Text="潜水时间 - u64" />
                            <TextBox Style="{StaticResource TextBoxInputStyle}" Text="MP0_TIME_UNDERWATER">
                                <TextBox.ToolTip>
                                    <TextBlock Text="数值单位为毫秒，例如：需修改成1小时则输入3600000" />
                                </TextBox.ToolTip>
                            </TextBox>

                            <TextBlock FontWeight="Bold" Text="步行时间 - u64" />
                            <TextBox Style="{StaticResource TextBoxInputStyle}" Text="MP0_TIME_WALKING">
                                <TextBox.ToolTip>
                                    <TextBlock Text="数值单位为毫秒，例如：需修改成1小时则输入3600000" />
                                </TextBox.ToolTip>
                            </TextBox>

                            <TextBlock FontWeight="Bold" Text="步行距离 - float" />
                            <TextBox Style="{StaticResource TextBoxInputStyle}" Text="MP0_DIST_WALKING">
                                <TextBox.ToolTip>
                                    <TextBlock Text="数值单位为米，例如：需修改成1千米则输入1000" />
                                </TextBox.ToolTip>
                            </TextBox>

                            <TextBlock FontWeight="Bold" Text="跑步距离 - float" />
                            <TextBox Style="{StaticResource TextBoxInputStyle}" Text="MP0_DIST_RUNNING">
                                <TextBox.ToolTip>
                                    <TextBlock Text="数值单位为米，例如：需修改成1千米则输入1000" />
